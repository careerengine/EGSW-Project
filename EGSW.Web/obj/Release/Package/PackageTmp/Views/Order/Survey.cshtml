@model EGSW.Web.Models.Orders.SurveryModel

@{
    ViewBag.Title = "Survey";
}

<h2>Eguttercleaning Survey</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.SurveryGuid)
    <div class="form-horizontal">
        <h4>Evaluation Of The Eguttercleaning Service</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        

        <div class="form-group">
           
            <div class="col-md-12" style="display:none;">
                Overall, how satisfied are you with the Eguttercleaning service? *

                <table cellspacing="0" cellpadding="5" border="0" id="entry_1578628282">
                    <tbody>
                        <tr aria-hidden="true">
                            <td class="ss-scalenumbers"></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_1" class="ss-scalenumber">1</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_2" class="ss-scalenumber">2</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_3" class="ss-scalenumber">3</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_4" class="ss-scalenumber">4</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_5" class="ss-scalenumber">5</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_6" class="ss-scalenumber">6</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_7" class="ss-scalenumber">7</label></td>
                            <td class="ss-scalenumbers"></td>
                        </tr>
                        <tr aria-label="Overall, how satisfied are you with the Eguttercleaning service?  Select a value from a range of 1,Not At All Satisfied, to 7,Extremely Satisfied,." role="radiogroup">
                            <td class="ss-scalerow ss-leftlabel"><div class="aria-todo" aria-hidden="true">Not At All Satisfied</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 1)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 2)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 3)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 4)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 5)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 6)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question1, 7)</div></td>

                            <td aria-hidden="true" class="ss-scalerow ss-rightlabel">Extremely Satisfied</td>
                        </tr>
                    </tbody>
                </table>


                @Html.ValidationMessageFor(model => model.Question1, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            
            <div class="col-md-12">
                How would you rate the cleanliness of the Eguttercleaning service? *

                <table cellspacing="0" cellpadding="5" border="0" id="entry_1578628282">
                    <tbody>
                        <tr aria-hidden="true">
                            <td class="ss-scalenumbers"></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_1" class="ss-scalenumber">1</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_2" class="ss-scalenumber">2</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_3" class="ss-scalenumber">3</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_4" class="ss-scalenumber">4</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_5" class="ss-scalenumber">5</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_6" class="ss-scalenumber">6</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_7" class="ss-scalenumber">7</label></td>
                            <td class="ss-scalenumbers"></td>
                        </tr>
                        <tr aria-label="Overall, how satisfied are you with the Eguttercleaning service?  Select a value from a range of 1,Not At All Satisfied, to 7,Extremely Satisfied,." role="radiogroup">
                            <td class="ss-scalerow ss-leftlabel"><div class="aria-todo" aria-hidden="true">Not At All Not At All Clean</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 1)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 2)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 3)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 4)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 5)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 6)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question2, 7)</div></td>

                            <td aria-hidden="true" class="ss-scalerow ss-rightlabel">Extremely Clean</td>
                        </tr>
                    </tbody>
                </table>
                @Html.ValidationMessageFor(model => model.Question2, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
           
            <div class="col-md-12">
                Please indicate how quickly the work was completed from the time you ordered the service to the time the gutter service was completed? *
                
                
                <table cellspacing="0" cellpadding="5" border="0" id="entry_1578628282">
                    <tbody>
                        <tr aria-hidden="true">
                            <td class="ss-scalenumbers"></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_1" class="ss-scalenumber">1</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_2" class="ss-scalenumber">2</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_3" class="ss-scalenumber">3</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_4" class="ss-scalenumber">4</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_5" class="ss-scalenumber">5</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_6" class="ss-scalenumber">6</label></td>
                            <td class="ss-scalenumbers"><label for="group_1578628282_7" class="ss-scalenumber">7</label></td>
                            <td class="ss-scalenumbers"></td>
                        </tr>
                        <tr aria-label="Overall, how satisfied are you with the Eguttercleaning service?  Select a value from a range of 1,Not At All Satisfied, to 7,Extremely Satisfied,." role="radiogroup">
                            <td class="ss-scalerow ss-leftlabel"><div class="aria-todo" aria-hidden="true">Not At All Quick</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 1)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 2)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 3)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 4)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 5)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 6)</div></td>
                            <td class="ss-scalerow"><div class="ss-scalerow-fieldcell"> @Html.RadioButtonFor(m => m.Question3, 7)</div></td>

                            <td aria-hidden="true" class="ss-scalerow ss-rightlabel">Extremely Quick</td>
                        </tr>
                    </tbody>
                </table>
                @Html.ValidationMessageFor(model => model.Question3, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
           
            <div class="col-md-12">
                Did the workers who performed the Eguttercleaning cleaning service offer you any additional "handyman" services? *
                

                <ul aria-label="Did the workers who performed the Eguttercleaning cleaning service offer you any additional &quot;handyman&quot; services?  " role="radiogroup" class="ss-choices">
                    <li class="ss-choice-item">
                        <label>
                            <span class="ss-choice-item-control goog-inline-block">
                                @Html.RadioButtonFor(m => m.Question4, "1")
                            </span>
                            <span class="ss-choice-label">Yes</span>
                        </label>
                    </li>
                    <li class="ss-choice-item">
                        <label>
                            <span class="ss-choice-item-control goog-inline-block">
                                @Html.RadioButtonFor(m => m.Question4, "2")
                            </span>
                            <span class="ss-choice-label">No</span>
                        </label>
                    </li>
                </ul>

                @Html.ValidationMessageFor(model => model.Question4, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </div>
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}