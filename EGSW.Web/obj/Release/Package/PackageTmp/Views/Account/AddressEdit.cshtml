@model AddressModel
@using EGSW.Web.Models.Customers;
@{
    Layout = "~/Views/Shared/_ColumnsTwo.cshtml";


}
@section left
{
    @Html.Action("CustomerNavigation", "Account", new { selectedTabId = CustomerNavigationEnum.Addresses })
}
@using (Html.BeginForm())
{
    <div class="page account-page address-edit-page">
        <div class="page-title">
            <h4>My account - Edit address</h4>
        </div>
        <div class="page-body">
            @{
                var validationSummary = Html.ValidationSummary(true);
            }
                        
            @{
                @Html.Partial("_CreateOrUpdateAddress", Model)
             }
            
        </div>
    </div>
}